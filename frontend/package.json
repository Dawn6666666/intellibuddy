{
  "name": "@intellibuddy/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,json,css,scss,md}\"",
    "clean": "rimraf dist node_modules/.vite"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@antv/x6-vue-shape": "^2.1.2",
    "@element-plus/icons-vue": "^2.3.2",
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.3",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.25",
    "marked": "^16.3.0",
    "marked-katex-extension": "^5.1.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-echarts": "^8.0.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "@types/node": "^20.19.21",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vue-tsc": "^3.0.7"
  }
}